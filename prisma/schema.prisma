generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Logs {
  id                 String    @id @default(cuid())
  horarioTratador    DateTime
  horarioAerador     DateTime
  horarioTemperatura DateTime?
  grausTemp          Decimal
}

model User {
  id        String    @id @default(cuid())
  email     String    @unique
  username  String    @unique
  name      String?
  password  String
  createdAt DateTime? @default(now())
  updatedAt DateTime?
}
